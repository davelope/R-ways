for (i in 1:5) {print("Hello")}
for (i in 1:5) {print(i)}

#ini kalau yg diprint abjadnya
abjad <- LETTERS[1:10]
for (i in abjad) {print (abjad)}

#ini kalau yg diprint i nya
abjad <- LETTERS[1:10]
for (i in abjad) {print (i)}

#########################

#ini nyoba dibuat matrix
dfabjad <- matrix(abjad,2,5)
dfabjad

